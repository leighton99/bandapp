<h1>Editing Band</h1>

<%= form_for(@band) do |f| %>
 
  <p>
		<%= f.label :name %><br>
		<%= f.text_field :name %><br>
	</p>
	<p>
		<%= f.label :genre %><br>
		<%= f.text_field :genre %><br>
	</p>
	
		<p>
		<%= f.label :explicit_lyrics %><br>
		<%= f.text_field :explicit_lyrics %><br>
	</p>

   <p>
    <%= f.submit 'Edit Band'%>
  </p>

<% end %>

<%= link_to 'Band Lists', bands_path %> | <%= link_to 'Delete Band', @band, method: :delete, data: { confirm: 'Are you sure want to delete this band?' } %>